<div class="edit-page">
  <div
    id="map"
    data-markers="<%= @markers.to_json %>" >
  </div>
  <div class="story-edit-div">
    <%= link_to edit_story_details_path(@story), remote: true do %>
      <h4 id="story-title" class="text-center"><%= @story.title %></h4>
    <% end %><hr>

    <div class="full-list">
      <ul id="blocks-list">
        <% @blocks.sort_by{|b| b.position}.each do |block| %>
          <li class="block-item"id="block-<%=block.id%>">
            <%= render 'block_list', block: block %>
            <% if block.events %>
              <%= render 'event_list', block: block %>
            <% end %>
          </li>
        <% end %>
      </ul>
      <%= button_to "Add a block", new_story_block_path(@story),
          :class => "btn btn-success center-block", :method => :get %>
        <hr>
    </div>
  </div>
</div>



